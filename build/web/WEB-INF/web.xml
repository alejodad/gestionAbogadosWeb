<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>User</servlet-name>
        <servlet-class>control.User</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Agregrar</servlet-name>
        <servlet-class>control.Agregrar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>AgregarPosth</servlet-name>
        <servlet-class>control.AgregarPosth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Eliminar</servlet-name>
        <servlet-class>control.Eliminar</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>ModificarPosth</servlet-name>
        <servlet-class>control.ModificarPosth</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>Gestion</servlet-name>
        <servlet-class>control.Gestion</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>User</servlet-name>
        <url-pattern>/user</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Agregrar</servlet-name>
        <url-pattern>/agregar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>AgregarPosth</servlet-name>
        <url-pattern>/agregarPosth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Eliminar</servlet-name>
        <url-pattern>/eliminar</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>ModificarPosth</servlet-name>
        <url-pattern>/modificarPosth</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>Gestion</servlet-name>
        <url-pattern>/gestion</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
